    "winston": "^3.16.0"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.2",
    "jest": "^29.7.0",
    "mocha": "^10.7.3",
    "prop-types": "^15.8.1"
  }
