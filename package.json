{
  "name": "type-scaf",
  "version": "0.1.0",
  "description": "Scaffold typescript types to react components",
  "main": "index.js",
  "bin": {
    "type-scaf": "./index.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --silent=false --verbose",
